    "\n",
    "def player_power(player_dict,pokedex, playerID):\n",
    "    \n",
    "    for player in player_dict:\n",
    "        \n",
    "        \n",
    "        if playerID == player_dict[player]['player_id']:\n",
    "            player_sum=[]\n",
    "            for poke in player_dict[player]['player_pokemon']: #\n",
    "                player_sum.append(poke['hp'])\n",
    "                player_sum.append(poke['defense'])\n",
    "                player_sum.append(poke['special_attack'])\n",
    "                player_sum.append(poke['special_defense'])\n",
    "                player_sum.append(poke['speed'])\n",
    "                \n",
    "            print player_dict[playerID]['player_name'],\"'s power is\", sum(player_sum)\n",
    "    Sum = sum(player_sum)\n",
    "    return Sum\n",
    "        \n",
    "player_power(players,pokemon,2)\n",
    "\n",
    "# Example output:\n",
    "#\n",
    "# Ash's power is 57.\n",
    "# 57\n",
