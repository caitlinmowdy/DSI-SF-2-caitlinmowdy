   ]
  },
  {
   "cell_type": "code",
   "execution_count": 377,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "yelp_users['recent'] = [1 if (2012 in x) |(2013 in x) | (2014 in x ) else 0 for x in yelp_users['elite']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 378,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "yelp_users['past'] = [1 if (2005 in x) | (2006 in x )| (2007 in x)|(2008 in x) | (2009 in x )| (2010 in x)| (2011 in x) else 0 for x in yelp_users['elite']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 379,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "yelp_users['top_dogs'] = yelp_users['recent'] + yelp_users['past']\n",
    "yelp_users['top_dogs'] = yelp_users['top_dogs'].map(lambda x: 1 if x == 2 else 0)"
   ]
  },
