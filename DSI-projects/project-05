    "def same(x):\n",
    "    x = x.iloc[0]\n",
    "    if len(x) == 0:\n",
    "        return np.nan\n",
    "    else:\n",
    "        return x\n",
    "\n",
    "yelp_bus_wide = pd.pivot_table(yelp_bus,\n",
    "                               index=['business_id', 'name', 'review_count', 'city', 'stars', 'categories'],\n",
