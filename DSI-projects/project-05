    "def same(x):\n",
    "    x = x.iloc[0]\n",
    "    if len(x) == 0:\n",
    "        return np.nan\n",
    "    else:\n",
    "        return x\n",
    "\n",
    "yelp_ci_wide = pd.pivot_table(yelp_ci,\n",
    "                               index=['business_id'],\n",
    "                               columns=['variable'],\n",
    "                               values='value',\n",
    "                               aggfunc=same).reset_index()"
   ]
  },
