   "source": [
    "# here we are finding our users that have made atlease 50 reviews. \n",
    "# We are doing this because people who are more consistent about reviewing tend to give more honest reiviews\n",
    "# people who only review once or twice are usually  giving an extream review\n",
    "\n",
    "users = yelp_rev.groupby(['business_id'])[['user_id']].count().sort_values('user_id',ascending = False)\n",
    "top_users = users[users['user_id'] >= 50]\n",
    "top_users.shape"
   ]
