   "source": [
    "service = []\n",
    "foods = []\n",
    "for c in yelp_rt.columns:\n",
    "    \n",
    "    if 'service' in c or 'staff' in c:\n",
    "        service.append(c)\n",
    "        \n",
    "    elif 'food' in c:\n",
    "        foods.append(c)\n",
    "print len(foods), len(service)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 176,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "yelp_rt['serv'] = 0\n",
    "yelp_rt['food'] = 0\n",
    "\n",
    "for s in service:\n",
    "    yelp_rt['serv'] = yelp_rt['serv'] + np.where(yelp_rt[s] > 0, 1, 0)\n",
    "for f in foods:\n",
    "    yelp_rt['food'] = yelp_rt['food'] + np.where(yelp_rt[f] > 0, 1, 0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 199,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "yelp = yelp_rt.groupby(['business_id_x','stars']).mean()\n",
    "yelp = yelp.reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 200,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "yelp['service_over_food'] =  np.where(yelp['serv'] > yelp['food'], 1, 0)"
   ]
  },
