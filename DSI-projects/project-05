    "top_users = top_users.reset_index()\n",
    "top_users.columns=['id','num_rev']\n",
    "\n",
    "user_list = list(top_users.id)\n",
    "\n",
    "user_mask = yelp_rev['business_id'].isin(user_list)\n",
    "\n",
    "cons_users = yelp_rev[user_mask]\n",
    "cons_users.head(2)"
   ]
